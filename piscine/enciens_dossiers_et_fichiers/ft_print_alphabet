#include <unistd.h>

int i = 'a';
	
void ft_print_alphabet(void){

while (i <= 122){
	
	ft_putchar(i);
	i ++;
	
	}
}

void	ft_putchar(char c){
	

write (1, &c, 1);	

}	

void main(){

ft_print_alphabet();

}

